<body>

<div data-role="header">
  <a id="link-dashboard" data-icon="arrow-l">Dashboard</a>
  <h1>Import Conversations</h1>
</div>

<div data-role="content">
  <form id="new-conversation-1-form">
    <div data-role="fieldcontain" class="ui-field-contain">
      <label for="target" class="ui-hidden-accessible">Name</label>
      <input type="text" name="target" id="target" value="" placeholder="Target's Name" />
    </div>
    <fieldset data-role="controlgroup" data-type="horizontal">
      <input type="radio" name="radio-choice-1" id="radio-choice-1" value="choice-1" checked="checked" />
      <label for="radio-choice-1">F</label>
      <input type="radio" name="radio-choice-1" id="radio-choice-2" value="choice-2"  />
      <label for="radio-choice-2">M</label>
    </fieldset>
    <div data-role="fieldcontain" class="ui-field-contain">
      <button type="submit" id="new-conversation-button">+ New</button>
    </div>
  </form>
</div>

</body>
</html>